{
  "name": "simplestyle-js-plugin-postcss",
  "version": "0.1.0",
  "description": "PostCSS integration with simplestyle-js to provide helpful utility and additional features to the zero-dependency simplestyle base",
  "main": "dist/commonjs/index.js",
  "typings": "dist/commonjs/index.d.ts",
  "types": "dist/commonjs/index.d.ts",
  "module": "dist/esm/index.js",
  "js:next": "dist/esm/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benduran/simplestyle-plugin-postcss.git"
  },
  "scripts": {
    "build:commonjs": "tsc -p ./tsconfig.json",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build": "run-s clean build:commonjs build:esm",
    "clean": "rimraf ./dist",
    "test": "jest --env jsdom",
    "test:watch": "jest --env jsdom --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "CSS-in-JS",
    "CSS",
    "Style",
    "Styled",
    "Simple",
    "Plugin",
    "PostCSS",
    "Autoprefixer"
  ],
  "author": "Benjamin Duran <stratodyne@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/jsdom": "^12.2.4",
    "autoprefixer": "9.7.3",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jsdom": "^15.2.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-react": "^4.1.0",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@types/autoprefixer": "^9.6.1",
    "postcss": "^7.0.23",
    "simplestyle-js": "^1.0.2"
  },
  "husky": {
    "pre-push": "npm test"
  }
}
