{
  "name": "simplestyle-js-plugin-postcss",
  "version": "2.0.0",
  "description": "PostCSS integration with simplestyle-js to provide helpful utility and additional features to the zero-dependency simplestyle base",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benduran/simplestyle-plugin-postcss.git"
  },
  "scripts": {
    "build:commonjs": "tsc -p ./tsconfig.build.json",
    "build": "run-s clean build:commonjs",
    "clean": "rimraf ./dist",
    "test": "jest --env jsdom",
    "test:watch": "jest --env jsdom --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "CSS-in-JS",
    "CSS",
    "Style",
    "Styled",
    "Simple",
    "Plugin",
    "PostCSS",
    "Autoprefixer"
  ],
  "author": "Benjamin Duran <stratodyne@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/jsdom": "^16.2.0",
    "autoprefixer": "9.7.5",
    "husky": "^4.2.3",
    "jest": "^25.2.7",
    "jsdom": "^16.2.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.8.2",
    "tslint": "^6.1.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-react": "^4.2.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/autoprefixer": "^9.7.2",
    "postcss": "^7.0.27",
    "simplestyle-js": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  }
}
